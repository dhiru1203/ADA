#include<iostream>
using namespace std;
int main(){
int n,i,ele,flag=-1;
cout<<"how many elements to enter:";
cin>>n;
int a[n];
cout<<"Enter Elements\n";
for(i=0;i<n;i++)
	cin>>a[i];
cout<<"Enter Element to search:";
cin>>ele;
for(i=0;i<n;i++){
	if(ele==a[i]){
		flag=0;
		cout<<"\nelement found at position "<<i+1;		
	}
	}
if(flag==-1)
cout<<"element not found";
return 0;
}
